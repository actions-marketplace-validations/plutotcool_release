{
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": ".",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  }
}
